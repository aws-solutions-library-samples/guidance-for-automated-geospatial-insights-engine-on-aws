{
	"name": "@agie/integration-tests",
	"version": "0.2.0",
	"description": "Framework wide integration tests.",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "npx tsc -b",
