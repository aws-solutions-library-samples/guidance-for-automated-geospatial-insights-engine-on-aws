{
	"name": "@arcade/lambda-invoker",
	"version": "1.0.0",
	"description": "Lambda Invoker",
	"author": "Dean Hart <deanhart@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "npx tsx esbuild.ts",
		"test": "npm run build && vitest run"
	},
	"dependencies": {
		"@aws-sdk/client-lambda": "~3",
		"@aws-sdk/util-utf8-node": "~3",
		"@aws-sdk/client-sso-oidc": "~3",
		"http-errors": "2.0.0",
		"js-yaml": "4.1.0",
		"readable-stream": "4.4.2"
	},
	"devDependencies": {
		"@tsconfig/node20": "~20",
		"@types/http-errors": "2.0.1",
		"@types/node": "~20",
		"@types/prettier": "~3",
		"@typescript-eslint/eslint-plugin": "~7",
		"@typescript-eslint/parser": "~7",
		"aws-sdk-client-mock": "~4",
		"esbuild": "~0",
		"eslint": "~9",
		"pino": "~8",
		"shx": "~0",
		"tsx": "~4",
		"typescript": "~5",
		"vitest": "~1",
		"vitest-mock-extended": "~1"
	},
	"private": false
}
