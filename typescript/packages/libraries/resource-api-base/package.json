		"@aws-sdk/lib-dynamodb": "~3",
		"@smithy/smithy-client": "~3",
		"@aws-sdk/types": "~3",
		"@aws-sdk/util-dynamodb": "~3",
		"@fastify/awilix": "~6",
		"@fastify/aws-lambda": "~5",
		"@fastify/cors": "~10",
		"@fastify/env": "~5",
		"@fastify/sensible": "~6",
		"@fastify/swagger": "~9",
		"@fastify/type-provider-typebox": "~5",
		"@sinclair/typebox": "~0",
		"@agie/dynamodb-utils": "workspace:*",
		"@agie/lambda-invoker": "workspace:*",
