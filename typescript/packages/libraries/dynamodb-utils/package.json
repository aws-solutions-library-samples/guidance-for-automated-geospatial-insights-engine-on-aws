		"pino": "~8",
		"pino-pretty": "~11"
	},
	"devDependencies": {
		"@tsconfig/node20": "~20",
		"@types/node": "~20",
		"eslint": "~9",
		"shx": "~0",
		"tsx": "~4",
		"typescript": "~5",
		"vitest": "~1",
		"vitest-mock-extended": "~1"
	},
	"private": false
