		"@aws-sdk/types": "3.535.0",
		"aws-cdk-lib": "2.136.1",
		"aws-lambda": "1.0.7",
		"constructs": "10.3.0",
		"lodash.chunk": "4.2.0",
		"octokit": "3.2.0 ",
		"shelljs": "0.8.5"
	},
	"devDependencies": {
		"@tsconfig/node20": "20.1.4",
		"@types/aws-lambda": "8.10.137",
		"@types/lodash.chunk": "4.2.9",
		"@types/node": "20.12.7",
		"@typescript-eslint/eslint-plugin": "7.6.0",
		"@typescript-eslint/parser": "7.6.0",
		"aws-cdk": "2.136.1",
