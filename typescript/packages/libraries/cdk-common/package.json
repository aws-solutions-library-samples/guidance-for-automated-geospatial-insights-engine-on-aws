{
	"name": "@arcade/cdk-common",
	"version": "1.0.0",
	"description": "CDK constructs and shared utilities",
	"author": "Pouya Bahavar <bahavare@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
