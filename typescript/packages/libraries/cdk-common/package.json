		"custom-resource-helper": "1.0.15",
		"esbuild": "0.20.2",
		"eslint": "9.0.0",
		"shx": "0.3.4",
		"tsx": "4.7.2",
		"typescript": "5.4.5",
		"vitest-mock-extended": "1.3.1",
		"vitest": "1.4.0"
