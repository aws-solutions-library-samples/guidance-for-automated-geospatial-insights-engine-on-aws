{
	"name": "@arcade/results",
	"description": "Arcade: Results module",
	"version": "1.0.0",
	"author": "Pouya Bahavar <bahavar@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "npx tsc -b",
		"prestart": "npx ts-node --esm --prefer-ts-exts src/localConfigGenerator.ts",
		"start": "rushx build && node -r dotenv/config dist/server.js",
		"test": "npm run build && vitest run --passWithNoTests"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "~3",
		"@aws-sdk/client-eventbridge": "~3",
		"@aws-sdk/lib-dynamodb": "~3",
		"@aws-sdk/client-s3": "~3",
		"@aws-sdk/smithy-client": "~3",
		"@aws-sdk/client-ssm": "~3",
		"@aws-sdk/client-sns": "~3",
		"@aws-sdk/types" : "~3",
		"@aws-sdk/client-lambda" : "~3",
		"@aws-sdk/util-stream-node" : "~3",
		"aws-lambda": "~1",
		"@fastify/awilix": "~5",
		"@fastify/aws-lambda": "~4",
		"@fastify/cors": "~9",
		"@fastify/env": "~4",
		"@fastify/sensible": "~5",
		"@fastify/swagger": "~8",
		"@fastify/type-provider-typebox": "~4",
		"@arcade/resource-api-base": "workspace:*",
		"@arcade/events": "workspace:*",
		"@arcade/dynamodb-utils": "workspace:*",
		"@arcade/cdk-common": "workspace:*",
		"@arcade/validators": "workspace:*",
		"@arcade/lambda-invoker": "workspace:*",
		"@arcade/clients": "workspace:*",
		"@sinclair/typebox": "~0",
		"awilix": "~10",
		"fastify-plugin": "~4",
		"fastify": "~4",
		"ulid": "~2",
		"aws-xray-sdk": "~3",
		"dayjs": "~1",
		"dayjs-plugin-utc" : "0.1.2"
	},
	"devDependencies": {
		"@tsconfig/node20": "~20",
		"@types/node": "~20",
		"@types/prettier": "~3",
		"@types/validator": "~13",
		"@typescript-eslint/eslint-plugin": "~7",
		"@typescript-eslint/parser": "~7",
		"@types/aws-lambda": "~8",
		"aws-lambda": "~1",
		"aws-sdk-client-mock": "~4",
		"dotenv": "~16",
		"esbuild": "~0",
		"eslint": "~9",
		"pino-pretty": "~11",
		"pino": "~8",
		"shx": "~0",
		"ts-node": "~10",
		"typescript": "~5",
		"vitest-mock-extended": "~1",
		"vitest": "~1"
	},
	"private": false
}
