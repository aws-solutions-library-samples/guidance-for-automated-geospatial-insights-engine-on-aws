{
  "name": "@arcade/regions-extension",
  "version": "0.0.1",
  "description": "Extending the functionality of the core region module to augment resources with external data.",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.js",
  "type": "module",
  "scripts": {
    "clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
    "build": "npx tsc -b",
    "test": "npm run build && vitest run",
    "prestart": "npx tsx src/localConfigGenerator.ts",
    "start": "rushx build && tsx src/server.ts",
    "cdk": "cdk"
  },
  "dependencies": {
    "@arcade/events": "workspace:*",
    "@arcade/clients": "workspace:*",
    "@arcade/lambda-invoker": "workspace:*",
    "@aws-sdk/client-lambda": "~3",
    "@fastify/awilix": "~5",
    "@fastify/aws-lambda": "~4",
    "awilix": "~10",
    "aws-lambda": "~1",
    "aws-xray-sdk": "~3",
    "dayjs": "~1",
    "dotenv": "~16",
    "fastify-plugin": "~4",
    "fastify": "~4",
    "ow": "~1",
    "pino-pretty": "~11",
    "pino": "~8"
  },
  "devDependencies": {
    "@tsconfig/node20": "~20",
    "@types/aws-lambda": "~8",
    "@types/node": "~20",
    "eslint": "~9",
    "shx": "~0",
    "tsx": "~4",
    "typescript": "~5",
    "vitest": "~1",
    "vitest-mock-extended": "~1"
  },
  "private": false
}
