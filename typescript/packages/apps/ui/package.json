{
	"name": "@agie/ui",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"predev": "npx tsx src/localConfigGenerator.ts",
		"dev": "vite",
		"build": "tsc",
		"bundle": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"deploy": "npm run predev && npm run bundle && ./upload-to-s3.bash",
		"preview": "vite preview"
	},
	"dependencies": {
		"react": "~18",
		"react-dom": "~18",
		"dayjs": "~1",
		"d3": "~7",
		"@cloudscape-design/global-styles": "~1",
		"@cloudscape-design/components": "~3",
		"@cloudscape-design/design-tokens": "~3",
		"react-router-dom": "~6",
		"maplibre-gl": "~4",
		"react-map-gl": "~7",
		"geojson": "~0",
		"@types/geojson": "~7946",
		"@aws-amplify/ui-react": "~6",
		"aws-amplify": "~6",
		"victory": "~37",
		"@aws/amazon-location-utilities-auth-helper": "~1",
		"@reduxjs/toolkit": "~2",
		"react-redux": "~9",
		"tsx": "~4"
	},
	"devDependencies": {
		"@agie/regions": "workspace:*",
		"@aws-sdk/client-ssm": "~3",
		"@aws-sdk/client-sts": "~3",
		"@types/react": "~18",
		"@types/react-dom": "~18",
		"@types/d3": "~7",
		"@typescript-eslint/eslint-plugin": "~8",
		"@typescript-eslint/parser": "~8",
		"@vitejs/plugin-react": "~4",
		"eslint": "~9",
		"eslint-plugin-react-hooks": "~4",
		"eslint-plugin-react-refresh": "~0",
		"typescript": "~5",
		"vite": "~5"
	}
}
